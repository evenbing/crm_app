{
 "name": "crm_app",
 "version": "0.0.1",
 "private": true,
 "author": "Justin Xu",
 "scripts": {
  "setup": "yarn install && react-native link",
  "android:gradlew": "chmod 755 android/gradlew",
  "ios:config": "cd node_modules/react-native/third-party/glog-0.3.4 && ../../scripts/ios-configure-glog.sh",
  "start": "node node_modules/react-native/local-cli/cli.js start",
  "android": "react-native run-android",
  "devMenu": "adb shell input keyevent 82",
  "reload": "adb shell input text 'RR'",
  "ios": "react-native run-ios",
  "iPhone6": "react-native run-ios --simulator 'iPhone 6'",
  "iPhone8": "react-native run-ios --simulator 'iPhone 8'",
  "iPhoneX": "react-native run-ios --simulator 'iPhone X'",
  "test": "jest",
  "clear": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && yarn install && yarn start -- --reset-cache",
  "build": "chmod 777 install.sh && ./install.sh"
 },
 "dependencies": {
  "antd-mobile-rn": "^2.3.1",
  "autobind-decorator": "^2.1.0",
  "js-rn-echarts": "^0.1.0",
  "md5": "^2.2.1",
  "mobx": "3.2.2",
  "mobx-react": "4.2.2",
  "moment": "^2.22.2",
  "native-base": "2.6.1",
  "prop-types": "^15.6.2",
  "react": "16.0.0-alpha.12",
  "react-native": "0.47.2",
  "react-native-image-crop-picker": "0.19.1",
  "react-native-linear-gradient": "^2.4.0",
  "react-native-modal": "^6.5.0",
  "react-native-progress": "^3.5.0",
  "react-native-svg": "5.4.1",
  "react-navigation": "1.0.0-beta.31",
  "rn-viewpager": "^1.2.9",
  "styled-components": "^3.3.3",
  "uuid": "^3.3.2",
  "xn-react-native-applets": "^0.1.1"
 },
 "devDependencies": {
  "babel-eslint": "^8.2.3",
  "babel-jest": "23.2.0",
  "babel-plugin-import": "^1.11.0",
  "babel-plugin-module-resolver": "^3.1.1",
  "babel-plugin-transform-decorators-legacy": "^1.3.5",
  "babel-preset-react-native": "4.0.0",
  "babel-preset-react-native-stage-0": "^1.0.1",
  "eslint": "^4.19.1",
  "eslint-config-airbnb": "^16.1.0",
  "eslint-plugin-import": "^2.12.0",
  "eslint-plugin-jsx-a11y": "^6.0.3",
  "eslint-plugin-react": "^7.9.1",
  "eslint-plugin-react-native": "^3.2.1",
  "jest": "23.2.0",
  "react-test-renderer": "16.0.0-alpha.12"
 },
 "jest": {
  "preset": "react-native"
 }
}
